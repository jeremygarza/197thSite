<p id="notice"><%= notice %></p>
  <div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="text-center m-t-lg">
            <h1>Contacts</h1>
            <div class="table-responsive">
              <table class="table table-hover table-bordered">
                <thead>
                  <tr>
                    <th>Position</th>
                    <th>Last name</th>
                    <th>First name</th>
                    <th>Rank</th>
                    <th>Phone</th>
                    <th colspan="3"></th>
                  </tr>
                </thead>
                <tbody>
    <% @support_contacts.each do |support_contact| %>
      <tr>
        <td><%= support_contact.position %></td>
        <td><%= support_contact.last_name %></td>
        <td><%= support_contact.first_name %></td>
        <td><%= support_contact.rank %></td>
        <td><%= support_contact.phone %></td>
        <td><%= link_to 'Show', support_contact %></td>
        <td><%= link_to 'Edit', edit_support_contact_path(support_contact) %></td>
        <td><%= link_to 'Destroy', support_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Support contact', new_support_contact_path %>
            </div>
          </div>
        </div>
      </div>
    </div>
