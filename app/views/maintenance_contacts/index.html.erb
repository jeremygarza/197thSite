<p id="notice"><%= notice %></p>

<h1>Listing Maintenance Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Rank</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maintenance_contacts.each do |maintenance_contact| %>
      <tr>
        <td><%= maintenance_contact.position %></td>
        <td><%= maintenance_contact.last_name %></td>
        <td><%= maintenance_contact.first_name %></td>
        <td><%= maintenance_contact.rank %></td>
        <td><%= maintenance_contact.phone %></td>
        <td><%= link_to 'Show', maintenance_contact %></td>
        <td><%= link_to 'Edit', edit_maintenance_contact_path(maintenance_contact) %></td>
        <td><%= link_to 'Destroy', maintenance_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Maintenance contact', new_maintenance_contact_path %>
